.sidebar-article-links
  ul.post-list
    h2 More Articles
    | {% for post in site.posts limit:10 %}
    li
      span.post-meta {{ post.date | date: "%b %-d" }}  
      h3
        a.post-link(href='{{ post.url | prepend: site.baseurl }}') {{ post.title }}
    | {% endfor %}
    
